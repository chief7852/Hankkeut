<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
	
<sqlMap namespace="manager">
	<select id="comparison" resultClass="String" parameterClass="managerVO">
		select * from manager 
		 where manager_id = #manager_id# and manager_pass = #manager_pass#
	</select>
	
	
</sqlMap>