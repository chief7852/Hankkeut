package vo;

public class InventoryVO {

	public int in_no         ;
	public int item_no       ;
	public String char_nickname ;
	public String in_allow      ;
	
	
	public int getIn_no() {
		return in_no;
	}
	public void setIn_no(int in_no) {
		this.in_no = in_no;
	}
	public int getItem_no() {
		return item_no;
	}
	public void setItem_no(int item_no) {
		this.item_no = item_no;
	}
	public String getChar_nickname() {
		return char_nickname;
	}
	public void setChar_nickname(String char_nickname) {
		this.char_nickname = char_nickname;
	}
	public String getIn_allow() {
		return in_allow;
	}
	public void setIn_allow(String in_allow) {
		this.in_allow = in_allow;
	}
	
	
	
	
}
